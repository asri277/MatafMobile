<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.matafmobile">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Verified_staff"></activity>
        <activity
            android:name=".Profile_ahli"
            android:parentActivityName=".MainActivity_ahli" />
        <activity
            android:name=".QR_generator"
            android:parentActivityName=".MainActivity_ahli" />
        <activity android:name=".MainActivity_ahli" />
        <activity
            android:name=".Register_ahli"
            android:parentActivityName=".Login_ahli"/>
        <activity
            android:name=".Login_ahli"
            android:parentActivityName=".Welcome" />
        <activity android:name=".Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Record"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Scanner"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Login"
            android:parentActivityName=".Welcome" />
        <activity
            android:name=".Register"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>